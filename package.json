{
  "name": "ExcelToTypeScript",
  "version": "0.6.5",
  "main": "main.js",
  "author": "zebin.ge",
  "contributors": [
    "long.shi"
  ],
  "description": "a tool for excel change to typescript",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build && electron . --eneable-logging",
    "package": "electron-forge package",
    "make": "ng build && electron-forge make",
    "ele": "electron .",
    "dist": "ng build && electron-builder --win --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "^13.3.8",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "angular": "^1.8.3",
    "angular-mat": "^1.0.0-alpha.1",
    "electron-fix": "^1.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.1.0",
    "node-xlsx": "^0.17.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.8",
    "@angular/cli": "~12.2.8",
    "@angular/compiler-cli": "~12.2.0",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.20.27",
    "electron": "^18.0.0",
    "electron-builder": "^22.14.5",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "i": "^0.3.7",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "node-xlsx": "^0.17.2",
    "npm": "^8.7.0",
    "typescript": "~4.3.5",
    "webpack-dev-server": "^3.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "myapp"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "build": {
    "appId": "com.electron.ExcelToTypeScript",
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "./icon.ico",
      "uninstallerIcon": "./icon.ico",
      "installerHeaderIcon": "./icon.svg",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
